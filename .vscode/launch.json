    "version": "0.2.0",
    "configurations": [
        {
            "name": "logs",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/__TEST__/loggy.go",
            "cwd": "${workspaceFolder}/__TEST__",
            "args": [
                "--debug",
                "--folder",
                " ./__TEST__/jobs"
            ]
        },
        {
            "name": "otsg dbg blink",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "--debug",
                "--dashboard",
                "--port",
                "3003",
                "--folder",
                "~/gr/otsg-marimo/src/jobs"
            ]
        },
        {
            "name": "otsg prod blink",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "--production",
                "--dashboard",
                "--folder",
                "~/gr/otsg-marimo/src/jobs"
            ]
        }
    ]
}